{
  "manifest_version": 3,

  "name": "Chrome-Sync",
  "description": "This extension refreshes the browser via io events",
  "version": "1.1",
  "background": {
    "page": "views/background.html"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "By DP, 4 DP"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}